{
  "CONFIG_PATH": "artifact/config",
  "DATA_PATH": "artifact/data",
  "LOG_PATH": "artifact/log",
  "CHECKPOINT_PATH": "artifact/checkpoint",
  "RESULT_PATH": "artifact/result",
  "MODEL_PATH": "artifact/model/",
  "ERROR_PATH": "artifact/error",

  "LIGHT_MODE": false,

  "AUGMENT_MODE": {
    "enabled": false
  },

  "L2_MODE": {
    "enabled": true,
    "adaptive": true,
    "lambda": 0.0005
  },

  "DROPOUT_MODE": {
    "enabled": false,
    "rate": 0.0
  },

  "OPTIMIZER": {
    "type": "sgd",
    "learning_rate": 0.05,
    "momentum": 0.9
  },

  "SCHEDULE_MODE": {
    "enabled": true,
    "type": "step",
    "milestones": [81, 122],
    "gamma": 0.1,
    "verbose": 1,
    "warmup_epochs": 5
  },

  "EARLY_STOP_MODE": {
    "enabled": false
  },

  "AVERAGE_MODE": {
    "enabled": true,
    "start_epoch": 150
  },

  "TTA_MODE": {
    "enabled": false
  },

  "MIXUP_MODE": {
    "enabled": true,
    "alpha": 0.2
  },

  "FROZEN_BN": true,

  "EPOCHS_COUNT": 164,
  "BATCH_SIZE": 128
}
